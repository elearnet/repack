{
  "name": "tester-win",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "win": "react-native run-windows --logging",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "test:windows": "jest --config jest.config.windows.js"
  },
  "dependencies": {
    "react": "19.0.0",
    "react-native": "catalog:repack-windows",
    "react-native-windows": "catalog:repack-windows"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.25.0",
    "@callstack/repack": "workspace:*",
    "@react-native-community/cli": "18.0.0",
    "@react-native-community/cli-platform-android": "18.0.0",
    "@react-native-community/cli-platform-ios": "18.0.0",
    "@react-native/babel-preset": "0.79.0",
    "@react-native/eslint-config": "0.79.0",
    "@react-native/metro-config": "0.79.0",
    "@react-native/typescript-config": "0.79.0",
    "@rnx-kit/jest-preset": "^0.1.17",
    "@types/jest": "^29.5.13",
    "@types/react": "^19.0.0",
    "@types/react-test-renderer": "^19.0.0",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "prettier": "2.8.8",
    "react-native-monorepo-tools": "^1.2.1",
    "react-test-renderer": "19.0.0",
    "typescript": "5.0.4",
    "webpack": "catalog:repack-windows"
  },
  "engines": {
    "node": ">=18"
  },
  "react-native-windows": {
    "init-windows": {
      "name": "TesterWin",
      "namespace": "TesterWin",
      "template": "cpp-app"
    }
  },
  "installConfig": {
    "hoistingLimits": "workspaces"
  }
}
